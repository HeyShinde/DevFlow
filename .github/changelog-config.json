{
  "categories": [
    {
      "title": "### üöÄ Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "### Added",
      "labels": ["added"]
    },
    {
      "title": "## [Unreleased]",
      "labels": ["added"]
    },
    {
      "title": "### Planned",
      "labels": ["planned"]
    },
    {
      "title": "## üêõ Fixes",
      "labels": ["fix", "bugfix"]
    },
    {
      "title": "## üìö Documentation",
      "labels": ["documentation", "docs"]
    },
    {
      "title": "## üîß Maintenance",
      "labels": ["chore", "refactor", "style", "test"]
    },
    {
      "title": "## üîí Security",
      "labels": ["security"]
    },
    {
      "title": "## üö® Breaking Changes",
      "labels": ["breaking"]
    }
  ],
  "ignore_labels": [
    "ignore"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "- No changes",
  "categories_template": "${{TITLE}}\n\n${{BODY}}",
  "change_template": "- ${{TITLE}}",
  "no_changes_template": "- No changes",
  "version_resolver": {
    "major": "Breaking Changes",
    "minor": "Features",
    "patch": "Fixes"
  },
  "label_extractor": [
    {
      "pattern": ".*",
      "target": "${{LABEL}}"
    }
  ],
  "duplicate_filter": {
    "pattern": ".*",
    "target": "${{TITLE}}"
  },
  "auto_sorter": {
    "asc": true,
    "target": "${{TITLE}}"
  },
  "pr_title": "chore: release ${{VERSION}}",
  "pr_body": "## üöÄ Release ${{VERSION}}\n\n${{CHANGELOG}}",
  "pr_branch": "release/${{VERSION}}",
  "pr_labels": ["release"],
  "pr_reviewers": [],
  "pr_assignees": [],
  "pr_milestone": "${{VERSION}}",
  "pr_draft": false,
  "pr_merge_method": "squash",
  "pr_delete_branch": true,
  "pr_merge_commit_message": "chore: release ${{VERSION}}",
  "pr_squash_commit_message": "chore: release ${{VERSION}}",
  "pr_rebase": false,
  "pr_allow_empty": false,
  "pr_allow_draft": false,
  "pr_allow_fork": false,
  "pr_allow_private": false,
  "pr_allow_public": true,
  "pr_allow_internal": true,
  "pr_allow_external": false,
  "pr_allow_restricted": false,
  "pr_allow_limited": false,
  "pr_allow_public_restricted": false,
  "pr_allow_public_limited": false,
  "pr_allow_public_internal": false,
  "pr_allow_public_external": false,
  "pr_allow_public_restricted_internal": false,
  "pr_allow_public_restricted_external": false,
  "pr_allow_public_restricted_limited": false,
  "pr_allow_public_limited_internal": false,
  "pr_allow_public_limited_external": false,
  "pr_allow_public_limited_restricted": false,
  "pr_allow_internal_restricted": false,
  "pr_allow_internal_limited": false,
  "pr_allow_internal_external": false,
  "pr_allow_internal_restricted_limited": false,
  "pr_allow_internal_restricted_external": false,
  "pr_allow_internal_limited_external": false,
  "pr_allow_internal_limited_restricted": false,
  "pr_allow_external_restricted": false,
  "pr_allow_external_limited": false,
  "pr_allow_restricted_limited": false,
  "pr_allow_public_internal_restricted": false,
  "pr_allow_public_internal_limited": false,
  "pr_allow_public_internal_external": false,
  "pr_allow_public_external_restricted": false,
  "pr_allow_public_external_limited": false,
  "pr_allow_internal_external_restricted": false,
  "pr_allow_internal_external_limited": false,
  "pr_allow_external_restricted_limited": false,
  "pr_allow_public_internal_external_restricted": false,
  "pr_allow_public_internal_external_limited": false,
  "pr_allow_public_internal_restricted_limited": false,
  "pr_allow_public_external_restricted_limited": false,
  "pr_allow_internal_external_restricted_limited": false,
  "pr_allow_public_internal_external_restricted_limited": false
} 